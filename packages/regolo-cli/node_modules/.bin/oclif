#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/matteo/Projects/regolo-client/ts-client/node_modules/.pnpm/oclif@4.22.79_@types+node@18.19.130/node_modules/oclif/bin/node_modules:/home/matteo/Projects/regolo-client/ts-client/node_modules/.pnpm/oclif@4.22.79_@types+node@18.19.130/node_modules/oclif/node_modules:/home/matteo/Projects/regolo-client/ts-client/node_modules/.pnpm/oclif@4.22.79_@types+node@18.19.130/node_modules:/home/matteo/Projects/regolo-client/ts-client/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/matteo/Projects/regolo-client/ts-client/node_modules/.pnpm/oclif@4.22.79_@types+node@18.19.130/node_modules/oclif/bin/node_modules:/home/matteo/Projects/regolo-client/ts-client/node_modules/.pnpm/oclif@4.22.79_@types+node@18.19.130/node_modules/oclif/node_modules:/home/matteo/Projects/regolo-client/ts-client/node_modules/.pnpm/oclif@4.22.79_@types+node@18.19.130/node_modules:/home/matteo/Projects/regolo-client/ts-client/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../oclif/bin/run.js" "$@"
else
  exec node  "$basedir/../oclif/bin/run.js" "$@"
fi
